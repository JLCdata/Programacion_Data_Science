    "def describir(datos):\n",
    "    # C√≥digo Aqu√≠\n",
    "    descripciones=[]\n",
    "    for fruta in datos:\n",
    "        text=\"{} {} es una fruta de tipo {} de color {}. {} pepas en su interior.\".format(\"La\" if fruta[\"nombre\"][-1]==\"a\" \n",
    "        else \"El\",fruta[\"nombre\"],fruta[\"tipo\"],fruta[\"color\"],\n",
    "        \"Presenta\" if fruta[\"pepas\"]==True  else \"No presenta\")\n",
    "        descripciones.append(text)\n",
    "    return descripciones\n",
    "\n",
    "\n",
    "def filtrar_por_pepa(datos, tiene_pepas=True):\n",
    "    # C√≥digo Aqu√≠\n",
    "    filtrados=[]\n",
    "    for fruta in datos:\n",
    "        if fruta[\"pepas\"]==tiene_pepas:\n",
    "            filtrados.append(fruta)\n",
    "    return filtrados\n",
    "\n",
    "\n",
    "def conteo_colores(datos):\n",
    "    # C√≥digo Aqu√≠\n",
    "    count_amarillo=0\n",
    "    count_naranjo=0\n",
    "    count_rojo=0\n",
    "    for fruta in datos:\n",
    "        if fruta[\"color\"]==\"amarillo\":\n",
    "            count_amarillo+=1\n",
    "        elif fruta[\"color\"]==\"naranjo\":\n",
    "            count_naranjo+=1\n",
    "        elif fruta[\"color\"]==\"rojo\":\n",
    "            count_rojo+=1\n",
    "    conteo={\"amarillo\": count_amarillo, \"naranjo\": count_naranjo, \"rojo\": count_rojo}\n",
    "    return conteo\n"
   ]
